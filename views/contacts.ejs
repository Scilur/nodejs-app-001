<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs') %>
<body>
  <%- include('./partials/nav.ejs') %>
  <h1>Contacts:</h1>
  <% const name = "Tryam Tryamych"; %>
  <ul>
    <li><h3><%= name %></h3></li>
    <% if (contacts.length > 0) { %>
      <% contacts.forEach(({link, name}) => { %>
        <li>
          <a href="<%= link %>"><%= name %></a></li>
        </li>
      <% }) %>
    <% } %>
  </ul>
</body>
</html>
